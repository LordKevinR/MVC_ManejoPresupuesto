@model TipoCuenta

@{
	ViewData["Title"] = "Borrar tipo cuenta";
}

<h1 class="text-center mb-5 mt-5">Borrar Tipo Cuenta</h1>

<h3 class="text-center mb-5 mt-5">Está seguro que desea borrar el tipo cuenta: @Model.Nombre?</h3>

<form class="text-center" asp-action="BorrarTipoCuenta">
	<input type="hidden" asp-for="Id" />
	<button type="submit" class="btn btn-borrar btn-danger">Borrar</button>
	<a class="btn btn-secondary" asp-action="Index">Cancelar</a>
</form>